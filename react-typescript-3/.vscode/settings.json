{
  "eslint.enable": true,
  "eslint.options": {
    "editor.rulers": {
      "no-debugger": "always"
    }
  },
  "editor.rulers": [100], // marcações de colunas
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    // "javascript",
    // "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
